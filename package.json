{
  "name": "vibezone-v2",
  "version": "2.0.0",
  "description": "VibeZone v2 - Electron Multi-Agent Dashboard",
  "main": "dist/main/main/index.js",
  "scripts": {
    "dev": "concurrently -k -n main,renderer,electron \"npm run dev:main\" \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "build": "npm run build:main && npm run build:renderer",
    "start": "electron .",
    "clean": "node -e \"const fs=require('fs');['dist'].forEach(d=>{if(fs.existsSync(d)){fs.rmSync(d,{recursive:true,force:true})}})\"",
    "typecheck": "tsc -p tsconfig.main.json --noEmit && tsc -p tsconfig.json --noEmit",
    "lint": "tsc -p tsconfig.main.json --noEmit && tsc -p tsconfig.json --noEmit",
    "postinstall": "electron-builder install-app-deps",
    "package": "npm run build && electron-builder",
    "rebuild": "npx @electron/rebuild -f -w node-pty"
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.19.1",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.0",
    "node-pty": "^1.0.0",
    "ssh-config": "^5.0.0",
    "ssh2": "^1.16.0",
    "three": "^0.183.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/ssh2": "^1.15.0",
    "@types/three": "^0.183.1",
    "@vitejs/plugin-react": "^4.3.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.0",
    "framer-motion": "^11.11.0",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "wait-on": "^9.0.4",
    "zustand": "^5.0.0"
  }
}
